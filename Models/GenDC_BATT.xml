<?xml version = '1.0' encoding = 'UTF-8' ?>
<PropertyList>
  <params>
    <value>instrumentation/overhead-panel/dc-amps/deflection-norm</value>
  </params>

  <path>GenDC_BATT.ac</path>

  <animation include="SimpleGaugeEmissionInclude.xml"/>
    
  <!-- +/- 42 degrees for full deflection -->
  <animation>
    <type>rotate</type>
    <object-name>Needle</object-name>
    <property alias="/params/value"/>
    <factor>-42</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.0148246</z-m>
    </center>
  </animation>

</PropertyList>
