<?xml version = '1.0' encoding = 'UTF-8' ?>
<PropertyList>
  <params>
    <altitude>instrumentation/altimeter[0]/indicated-altitude-ft</altitude>
    <subscale>instrumentation/altimeter[0]/setting-inhg</subscale>
  </params>

  <path>Altimeter.ac</path>

  <animation>
    <type>material</type>
    <object-name>Face</object-name>
    <object-name>Needle</object-name>
    <object-name>Altitude10000</object-name>
    <object-name>Altitude1000</object-name>
    <object-name>Altitude100</object-name>
    <object-name>Subscale1000</object-name>
    <object-name>Subscale100</object-name>
    <object-name>Subscale10</object-name>
    <object-name>Subscale1</object-name>
    <emission include="InstrumentLightEmissionInclude.xml"/>
  </animation>
    
  <animation>
    <type>rotate</type>
    <object-name>Needle</object-name>
    <property alias="../../params/altitude"/>
    <factor>-0.360</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>Altitude100</object-name>
    <property alias="../../params/altitude"/>
    <factor>-0.001</factor>
    <step>100</step>
    <scroll>10</scroll>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>Altitude1000</object-name>
    <property alias="../../params/altitude"/>
    <factor>-0.0001</factor>
    <step>1000</step>
    <scroll>10</scroll>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>Altitude10000</object-name>
    <property alias="../../params/altitude"/>
    <factor>-0.00001</factor>
    <step>10000</step>
    <scroll>10</scroll>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>Subscale1</object-name>
    <property alias="../../params/subscale"/>
    <factor>-3.385695187165775401</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>Subscale10</object-name>
    <property alias="../../params/subscale"/>
    <factor>-0.3385695187165775401</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>Subscale100</object-name>
    <property alias="../../params/subscale"/>
    <factor>-0.03385695187165775401</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>Subscale1000</object-name>
    <property alias="../../params/subscale"/>
    <factor>-0.003385695187165775401</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
      <type>pick</type>
      <object-name>SubscalePick.L</object-name>
      <visible>false</visible>
      <action>
        <button>0</button>
        <repeatable>true</repeatable>
        <binding>
          <command>property-adjust</command>
          <property alias="../../../../params/subscale"/>
          <step>-0.02953603158933859822</step>
          <min>26</min>
          <max>33</max>
          <wrap>1</wrap>
        </binding>
      </action>
      <action>
        <button>1</button>
        <repeatable>true</repeatable>
        <binding>
          <command>property-adjust</command>
          <property alias="../../../../params/subscale"/>
          <step>-0.2953603158933859822</step>
          <min>26</min>
          <max>33</max>
          <wrap>1</wrap>
        </binding>
      </action>
  </animation>

  <animation>
      <type>pick</type>
      <object-name>SubscalePick.R</object-name>
      <visible>false</visible>
      <action>
        <button>0</button>
        <repeatable>true</repeatable>
        <binding>
          <command>property-adjust</command>
          <property alias="../../../../params/subscale"/>
          <step>0.02953603158933859822</step>
          <min>26</min>
          <max>33</max>
          <wrap>1</wrap>
        </binding>
      </action>
      <action>
        <button>1</button>
        <repeatable>true</repeatable>
        <binding>
          <command>property-adjust</command>
          <property alias="../../../../params/subscale"/>
          <step>0.2953603158933859822</step>
          <min>26</min>
          <max>33</max>
          <wrap>1</wrap>
        </binding>
      </action>
  </animation>

  <animation>
      <type>pick</type>
      <object-name>SubscaleKnob</object-name>
      <visible>true</visible>
      <action>
        <button>3</button>
        <repeatable>true</repeatable>
        <binding>
          <command>property-adjust</command>
          <property alias="../../../../params/subscale"/>
          <step>-0.02953603158933859822</step>
          <min>26</min>
          <max>33</max>
          <wrap>1</wrap>
        </binding>
      </action>
      <action>
        <button>4</button>
        <repeatable>true</repeatable>
        <binding>
          <command>property-adjust</command>
          <property alias="../../../../params/subscale"/>
          <step>0.02953603158933859822</step>
          <min>26</min>
          <max>33</max>
          <wrap>1</wrap>
        </binding>
      </action>
  </animation>

</PropertyList>
