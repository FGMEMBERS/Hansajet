<?xml version = '1.0' encoding = 'UTF-8' ?>
<PropertyList>
  <params>
    <fuel>consumables/fuel/tank[0]/level-lb</fuel>
  </params>

  <path>FuelFus.ac</path>

  <animation>
    <type>noshadow</type>
  </animation>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>10</max-m>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>LargeHand</object-name>
    <property alias="../../params/fuel"/>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>20</ind>
        <dep>10</dep>
      </entry>
      <entry>
        <ind>880</ind>
        <dep>234</dep>
      </entry>
      <entry>
        <ind>1100</ind>
        <dep>274</dep>
      </entry>
    </interpolation>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
  </animation>

</PropertyList>

