<?xml version = '1.0' encoding = 'UTF-8' ?>
<PropertyList>
  <params>
    <value>instrumentation/aoa-indexer/aoa-deg</value>
  </params>

  <path>AOAIndicator.ac</path>

  <animation include="SimpleGaugeEmissionInclude.xml"/>
    
  <animation>
    <type>rotate</type>
    <object-name>Needle</object-name>
    <property alias="../../params/value"/>
    <factor>-6</factor>
    <min-deg>-150</min-deg>
    <max-deg>90</max-deg>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
  </animation>

</PropertyList>
