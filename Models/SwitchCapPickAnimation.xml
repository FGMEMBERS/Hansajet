<?xml version = '1.0' encoding = 'UTF-8' ?>
<PropertyList>
  <object-name>SwitchCap</object-name>
  <type>pick</type>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <condition>
        <not>
          <property alias="../../../../../../params/value"/>
        </not>
      </condition>
      <command>property-toggle</command>
      <property alias="../../../../params/unlock"/>
    </binding>
  </action>
</PropertyList>
